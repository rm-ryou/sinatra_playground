<h1>Todos</h1>

<table>
  <thead>
    <tr>
      <th>Todo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @todos.each do |todo| %>
      <tr>
        <td><%= todo.name %></td>
        <td><a href="#">Show</a></td>
        <td><a href="#">Edit</a></td>
        <td><a href="#">Destroy</a></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<a href="/todos/new">New Todo</a>
